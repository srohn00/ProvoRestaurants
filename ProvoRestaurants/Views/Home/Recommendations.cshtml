@{
    
}
@model IEnumerable<ProvoRestaurants.Models.AddRestaurant>
<div>
    <h2>Here are some suggested restaurants</h2>
    <table>
        <thead>
            <tr><th>Name</th><th>Restaurant Name</th><th>Favorite Dish</th><th>Phone #</th></tr>
        </thead>
        <tbody>
            @foreach (ProvoRestaurants.Models.AddRestaurant r in Model)
            {
                <tr>
                    <td>@r.Name</td>
                    <td>@r.RestaurantName</td>
                    <td>@r.FavoriteDish</td>
                    <td>@r.Phone</td>
                </tr>
            }
        </tbody>
    </table>
</div>